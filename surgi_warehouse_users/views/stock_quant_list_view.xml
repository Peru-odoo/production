<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Inherit Form View to Modify it -->
        <record id="stock_quant_users_inherit" model="ir.ui.view">
            <field name="name">stock.quant.users.inherit</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_form"/>
            <field name="arch" type="xml">

                <field name="location_id" position="after">
                    <field name="is_wh_user" invisible="1"/>
                </field>

            </field>
        </record>

        <!--Inherit Tree View-->
        <record id="stock_quant_tree_users_inherit" model="ir.ui.view">
            <field name="name">stock.quant.tree.users.inherit</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
            <field name="arch" type="xml">

                <field name="location_id" position="after">
                    <field name="is_wh_user" invisible="0" optional="hide"/>
                </field>

            </field>
        </record>


    </data>
</odoo>